{
  "name": "vite-awesome-svg-loader",
  "description": "Imports SVGs as source code, base64 and data URI. Preserves stroke width, replaces colors with currentColor or custom colors. Optimizes SVGs with SVGO. Creates SVG sprites.",
  "keywords": [
    "vite",
    "svg",
    "loader",
    "vite-svg",
    "vite-loader",
    "vite-plugin",
    "vue",
    "react",
    "vanilla",
    "sprite",
    "sprite-sheet",
    "svg-sprite",
    "sprites",
    "svg-sprites",
    "spritesheet",
    "spritesheets",
    "sprite-sheets",
    "svg-spritesheets",
    "svg-sprite-sheets",
    "preserve-line-width",
    "preserve-stroke-width",
    "currentColor",
    "vite-awesome-svg-loader",
    "svgo"
  ],
  "version": "2.1.0",
  "homepage": "https://github.com/matafokka/vite-awesome-svg-loader",
  "repository": "https://github.com/matafokka/vite-awesome-svg-loader",
  "license": "LGPL-2.1-or-later",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "types": "./index.d.ts"
    },
    "./integration-utils": {
      "import": "./integration-utils/index.mjs",
      "require": "./integration-utils/index.js",
      "types": "./integration-utils/index.d.ts"
    },
    "./integration-utils/styles.css": {
      "import": "./integration-utils/styles.css",
      "require": "./integration-utils/styles.css"
    },
    "./vue-integration": {
      "import": "./vue-integration/index.js",
      "require": "./vue-integration/index.cjs",
      "types": "./vue-integration/index.d.ts"
    },
    "./vanilla-integration": {
      "import": "./vanilla-integration/index.js",
      "require": "./vanilla-integration/index.cjs",
      "types": "./vanilla-integration/index.d.ts"
    },
    "./react-integration": {
      "import": "./react-integration/index.js",
      "require": "./react-integration/index.cjs",
      "types": "./react-integration/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "./integration-utils": [
        "./integration-utils/index.d.ts"
      ],
      "./vue-integration": [
        "./vue-integration/index.d.ts"
      ],
      "./vanilla-integration": [
        "./vanilla-integration/index.d.ts"
      ],
      "./react-integration": [
        "./react-integration/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "css-tree": "^2.3.1",
    "fs-extra": "^11.1.1",
    "imurmurhash": "github:jensyt/imurmurhash-js",
    "svgo": "^3.0.2",
    "svgson": "^5.3.1",
    "vite": "*"
  },
  "devDependencies": {
    "@types/css-tree": "^2.3.3"
  }
}